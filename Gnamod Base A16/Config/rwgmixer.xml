    <hub_layout name="ruralHub02">
      <township_type value="town"/>

      <street start_point="-160,0" end_point="160,0"/>
      <street start_point="0,-160" end_point="0,160"/>

      <lot min_x_y="7, 7" size="128,128" rotation_to_road="2" align="south_west"/>
      <lot min_x_y="7, -135" size="128,128" rotation_to_road="0" align="north_west"/>

      <lot min_x_y="-135, 7" size="128,128" rotation_to_road="3" align="south_east"/>
      <lot min_x_y="-135, -135" size="128,128" rotation_to_road="1" align="north_east"/>
    </hub_layout>
  </hub_layouts>
	
<!--
	GENERATOR MODULE TYPES
	    AbsoluteOutput
        Add
        BiasOutput
        Blend
        ClampOutput
        CurveOutput
        DisplaceInput
        ExponentialOutput
        InvertInput
        InvertOutput
        LargerOutput
        Multiply
        Power
        RotateInput
        ScaleBiasOutput
        ScaleInput
        ScaleOutput
        Select
        SmallerOutput
        Terrace
        TranslateInput
        Billow
        Checkerboard
        Constant
        FastBillow
        FastNoise
        FastNoiseBasis
        FastRidgedMultifractal
        FastTurbulence
        GradientNoiseBasis
        RidgedMultifractal
        Turbulence
        Voronoi
	
-->

<terrain_generators>
<terrain_generator                        name="vanilla" use_old_final="false">

<!-- Base Flat Terrain-->
<module name="Base"                       type="RidgedMultifractal" seed_additive="5">
<property name="power"                    value="10"/>
<property name="frequency"                value="0.25"/>
<property name="roughness"                value="5"/>
</module>

<module name="BaseFilter"                 type="ScaleBiasOutput">
<property name="sourceModule"             value="Base"/>
<property name="scale"                    value="0.15"/>
<property name="bias"                     value="0.07"/>
</module>

<module name="WorldBase"                  type="ClampOutput">
<property name="sourceModule"             value="BaseFilter"/>
</module>

<!-- Crater Lakes -->
<module name="CraterLakesBase"            type="RidgedMultifractal">
<property name="frequency"                value="0.65"/>
<property name="roughness"                value="5"/>
</module>

<module name="CraterLakesMod"             type="ScaleBiasOutput">
<property name="sourceModule"             value="CraterLakesBase"/>
<property name="scale"                    value="0.40"/>
<property name="bias"                     value="0.30"/>
</module> 

<module name="CraterLakes"                type="ClampOutput">
<property name="sourceModule"             value="CraterLakesMod"/>
</module>

<!-- Lake Delta -->
<module name="LakeDeltaBase"              type="FastBillow">
<property name="frequency"                value="0.035"/>
</module>

<module name="LakeDeltaMod"               type="ScaleBiasOutput">
<property name="sourceModule"             value="LakeDeltaBase"/>
<property name="scale"                    value="0.42"/>
<property name="bias"                     value="0.40"/>
</module>

<module name="LakeDelta"                  type="FastTurbulence">
<property name="sourceModule"             value="LakeDeltaMod"/>
<property name="power"                    value="15"/>
<property name="frequency"                value="0.40"/>
<property name="roughness"                value="5"/>
</module>  
  
<!-- Alien Hills -->
<module name="AlienHillsBase"             type="FastBillow">
<property name="frequency"                value="0.095"/>
</module>

<module name="AlienHillsMod"              type="ScaleBiasOutput">
<property name="sourceModule"             value="AlienHillsBase"/>
<property name="scale"                    value="0.42"/>
<property name="bias"                     value="0.30"/>  
</module>

<module name="AlienHills"                 type="FastTurbulence">
<property name="sourceModule"             value="AlienHillsMod"/>
<property name="power"                    value="20"/>
<property name="frequency"                value="0.40"/>
<property name="roughness"                value="5"/>
</module>

<!-- Continental Mainland -->
<module name="ContinentsBase"             type="FastBillow">
<property name="frequency"                value="0.075"/>
</module>

<module name="ContinentsMod"              type="ScaleBiasOutput">
<property name="sourceModule"             value="ContinentsBase"/>
<property name="scale"                    value="0.42"/>
<property name="bias"                     value="0.25"/>  
</module>

<module name="Continents"                 type="FastTurbulence">
<property name="sourceModule"             value="ContinentsMod"/>
<property name="power"                    value="2"/>
<property name="frequency"                value="0.40"/>
<property name="roughness"                value="5"/>
</module>    
  
<!-- Rocky Rivers -->
<module name="RockyRiverBase"             type="FastBillow">
<property name="frequency"                value="0.075"/>
</module>

<module name="RockyRiverMod"              type="ScaleBiasOutput">
<property name="sourceModule"             value="RockyRiverBase"/>
<property name="scale"                    value="0.49"/>
<property name="bias"                     value="0.32"/>  
</module>

<module name="RockyRiver"                 type="FastTurbulence" seed_additive="5">
<property name="sourceModule"             value="RockyRiverMod"/>
<property name="power"                    value="5"/>
<property name="frequency"                value="1.10"/>
<property name="roughness"                value="1"/>
</module>

<module name="MountainsBase"              type="FastNoise">
<property name="frequency"                value="1.10"/>
</module>
  
<!-- Mountains -->
<module name="MountainsMod"               type="FastBillow">
<property name="sourceModule"             value="MountainsBase"/>
<property name="frequency"                value="1.50"/>
</module>

<module name="MountainsScale"             type="ScaleBiasOutput">
<property name="sourceModule"             value="MountainsMod"/>
<property name="scale"                    value="0.50"/>
<property name="bias"                     value="0.35"/>
</module>

<module name="Mountains"                  type="Select">
<property name="controlModule"            value="MountainsScale"/>
<property name="sourceModule1"            value="MountainsScale"/>
<property name="sourceModule2"            value="Continents"/>
<property name="sourceModule3"            value="MountainsScale"/>
<property name="sourceModule4"            value="MountainsScale"/>
<property name="sourceModule5"            value="MountainsScale"/>
<property name="bounds"                   value="0, 1"/>
<property name="edgeFalloff"              value="2.5"/>
</module>
 
<!-- Cliffs -->
<module name="CliffsBase"                 type="FastBillow">
<property name="frequency"                value="1.10"/>
</module>

<module name="CliffsMod"                  type="FastBillow">
<property name="sourceModule"             value="CliffsBase"/>
<property name="frequency"                value="1.10"/>
</module>

<module name="CliffsScale"                type="ScaleBiasOutput">
<property name="sourceModule"             value="CliffsMod"/>
<property name="scale"                    value="0.40"/>
<property name="bias"                     value="0.30"/>
<property name="bounds"                   value="-60, 110"/>
</module>

<module name="Cliffs"                     type="Select">
<property name="controlModule"            value="AlienHills"/>
<property name="sourceModule1"            value="CliffsScale"/>
<property name="sourceModule2"            value="WorldBase"/>
<property name="sourceModule3"            value="AlienHills"/>
<property name="sourceModule4"            value="CliffsScale"/>
<property name="sourceModule5"            value="CliffsScale"/>
<property name="bounds"                   value="0, 1"/>
<property name="edgeFalloff"              value="0.25"/>
</module>

<!-- Final Selecting-->
<module name="Final"                      type="Select">
<property name="controlModule"            value="Cliffs"/>
<property name="sourceModule1"            value="Mountains"/>
<property name="sourceModule2"            value="Cliffs"/>
<property name="sourceModule3"            value="Continents"/>
<property name="sourceModule4"            value="WorldBase"/>
<property name="sourceModule5"            value="CraterLakes"/>
<property name="sourceModule6"            value="LakeDelta"/>
<property name="sourceModule7"            value="Continents"/>
<property name="sourceModule8"            value="WorldBase"/>
<property name="sourceModule9"            value="Mountains"/>
<property name="sourceModule10"           value="WorldBase"/>
<property name="bounds"                   value="0, 1"/>
<property name="edgeFalloff"              value="2.5"/>
</module>
  
<!-- Final Scaling-->
<module name="FinalScale"                 type="ScaleBiasOutput">
<property name="sourceModule"             value="Final"/>
<property name="scale"                    value="250"/>
<property name="bias"                     value="-1"/>
</module>
  
<!-- Final Processing -->
<module name="WorldBuilding"              type="ClampOutput">
<property name="sourceModule"             value="FinalScale"/>
<property name="bounds"                   value="-26,255"/>
</module>

<!-- Final Output-->
<output module="WorldBuilding"/>
			
</terrain_generator>
</terrain_generators>

<!-- Biome Distribution -->
<biome_generators>
  
<biome_generator                          name="vanilla">
<module name="BaseNoise"                  type="Voronoi">
<property name="frequency"                value="0.00075"/>
<property name="displacement"             value="0.5"/>
</module>

<module name="BaseNoiseMod"               type="BiasOutput">
<property name="sourceModule"             value="BaseNoise"/>
<property name="bias"                     value="0.55"/>
</module>
			
<module name="BiomeDither"                type="FastTurbulence">
<property name="sourceModule"             value="BaseNoiseMod"/>
<property name="frequency"                value="0.25"/>
<property name="power"                    value="8"/>
<property name="roughness"                value="5"/>
</module>
			
<module name="BiomeJiggle"                type="FastTurbulence">
<property name="sourceModule"             value="BiomeDither"/>
<property name="frequency"                value="0.0015"/>
<property name="power"                    value="400"/>
<property name="roughness"                value="1"/>
</module>
			
<module name="BiomeBuilding"              type="ClampOutput">
<property name="sourceModule"             value="BiomeJiggle"/>
<property name="bounds"                   value="0,1"/>
</module>
			
<output module="BiomeBuilding"/>
</biome_generator>
  
<biome_generator                          name="test">
<module name="BaseNoise"                  type="FastNoise">
<property name="frequency"                value="0.0004"/>
<property name="displacement"             value="0.5"/>
</module>

<module name="BaseNoiseMod"               type="BiasOutput">
<property name="sourceModule"             value="BaseNoise"/>
<property name="bias"                     value="0.55"/>
</module>
			
<module name="BiomeDither"                type="FastTurbulence">
<property name="sourceModule"             value="BaseNoiseMod"/>
<property name="frequency"                value="0.25"/>
<property name="power"                    value="8"/>
<property name="roughness"                value="5"/>
</module>
			
<module name="BiomeJiggle"                type="FastTurbulence">
<property name="sourceModule"             value="BiomeDither"/>
<property name="frequency"                value="0.0018"/>
<property name="power"                    value="300"/>
<property name="roughness"                value="1"/>
</module>
			
<module name="BiomeBuilding"              type="ClampOutput">
<property name="sourceModule"             value="BiomeJiggle"/>
<property name="bounds"                   value="0,1"/>
</module>
			
<output module="BiomeBuilding"/>
</biome_generator>  
  
</biome_generators>

<!-- Biome Intensity -->
<biome_intensity_generators>
<biome_intensity_generator name="vanilla"> 
<module name="voronoi" type="Voronoi">
<property name="frequency" value="0.0008"/>
<property name="displacement" value="0.5"/>
<property name="distanceEnabled" value="true"/>
</module>

<module name="biasOutput" type="BiasOutput">
<property name="sourceModule" value="voronoi"/>
<property name="bias" value="0.5"/>
</module>
		
<module name="biomeDither" type="FastTurbulence">
<property name="sourceModule" value="biasOutput"/>
<property name="frequency" value="0.25"/>
<property name="power" value="8"/>
<property name="roughness" value="5"/>
</module>
			
<module name="biomeJiggle" type="FastTurbulence">
<property name="sourceModule" value="biomeDither"/>
<property name="frequency" value="0.0016"/>
<property name="power" value="400"/>
<property name="roughness" value="1"/>
</module>
			
<module name="clampOutput" type="ClampOutput">
<property name="sourceModule" value="biomeJiggle"/>
<property name="bounds" value="0,1"/>
</module>
			
<output module="clampOutput"/>
</biome_intensity_generator>
</biome_intensity_generators>

<biome_spawn_rules>
<!-- Dummy Biomes (Will be overwritten completely and serve to thin out the Overlay Biomes)           -->	
<biome_spawn_rule name="forest">        <biome_generator range="0.20,0.30"/>          </biome_spawn_rule>
<biome_spawn_rule name="forest">        <biome_generator range="0.30,0.40"/>          </biome_spawn_rule>
<biome_spawn_rule name="forest">        <biome_generator range="0.60,0.70"/>          </biome_spawn_rule>
<biome_spawn_rule name="forest">        <biome_generator range="0.80,0.90"/>          </biome_spawn_rule>
<!-- Grassland Overlay (Primary Biome Paint)                                                          -->
<biome_spawn_rule name="plains">        <terrain_generator range="-60,255"/>          </biome_spawn_rule>
<!-- Spot Biomes (Biomes that are drawn in spots but are overwritten by Mountain Paints)              -->
<biome_spawn_rule name="forest">        <biome_generator range="0.00,0.10"/>          </biome_spawn_rule>
<biome_spawn_rule name="wasteland">     <biome_generator range="0.40,0.50"/>          </biome_spawn_rule>
<biome_spawn_rule name="snow">          <biome_generator range="0.50,0.60"/>          </biome_spawn_rule>
<biome_spawn_rule name="pine_forest">   <biome_generator range="0.70,0.80"/>          </biome_spawn_rule>
<biome_spawn_rule name="burnt_forest">  <biome_generator range="0.90,1.00"/>          </biome_spawn_rule>
<!-- Mountain Paints (Biomes that make up the alpine mountains on the map                             -->
<biome_spawn_rule name="forest">        <terrain_generator range="90,255"/>           </biome_spawn_rule> 
<biome_spawn_rule name="pine_forest">   <terrain_generator range="100,255"/>          </biome_spawn_rule>  
<biome_spawn_rule name="snow">          <terrain_generator range="125,255"/>          </biome_spawn_rule>
<biome_spawn_rule name="city">          <terrain_generator range="150,255"/>          </biome_spawn_rule>
<!-- Water Overlay (Primary Water Biome for Biomes that require a nice water edge)                    -->  
<biome_spawn_rule name="water">         <terrain_generator range="-60,2"/>            </biome_spawn_rule>
<!-- Desert Spot Biome (Desert requires its own Mountain Paint)                                       -->
<biome_spawn_rule name="desert">        <biome_generator range="0.10,0.20"/>          </biome_spawn_rule>
<!-- Water Overlay (Desert requires a new water paint underwater)                                     -->
<biome_spawn_rule name="water">         <terrain_generator range="-60,-5"/>           </biome_spawn_rule>  
<!-- Radiated Biome Barrier                                                                           -->
<biome_spawn_rule name="radiated">      <distance_from_center range="10000,12000"/>   </biome_spawn_rule>
  
  
  
